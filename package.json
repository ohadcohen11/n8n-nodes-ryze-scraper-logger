{
	"name": "n8n-nodes-ryze-scraper-logger",
	"version": "0.1.0",
	"description": "n8n node for logging Ryze scraper execution metrics to MySQL database",
	"license": "MIT",
	"homepage": "",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"ryze",
		"mysql",
		"scraper",
		"logger"
	],
	"author": {
		"name": "Ryze Beyond",
		"email": "ohad.cohen@ryzebeyond.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ohadcohen11/n8n-nodes-ryze-scraper-logger.git"
	},
	"engines": {
		"node": ">=20.15"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "echo 'Skipping lint'",
		"lint:fix": "echo 'Skipping lint'",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/GithubIssuesApi.credentials.js",
			"dist/credentials/GithubIssuesOAuth2Api.credentials.js",
			"dist/credentials/MySqlApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/GithubIssues/GithubIssues.node.js",
			"dist/nodes/Example/Example.node.js",
			"dist/nodes/RyzeScraperLogger/RyzeScraperLogger.node.js"
		]
	},
	"dependencies": {
		"mysql2": "^3.6.0"
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@types/node": "^22.10.2",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
